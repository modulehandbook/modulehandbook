<p id="notice"><%= notice %></p>

<h3><%= content_for(:title, "Versions")</h3>

<br><br>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">item_type</th>
      <th scope="col">item_id</th>
      <th scope="col">event</th>
      <th scope="col">whodunnit</th>
      <th scope="col">created_at</th>
      <th scope="col">object_changes</th>
    </tr>
  </thead>

  <tbody>
    <% @versions.each do |version| %>
      <tr >
        <td ><%=version.item_type %></td>
        <td ><%=version.item_id %></td>
        <td ><%=version.event %></td>
        <td ><%=version.whodunnit %></td>
        <td ><%=version.created_at %></td>
        <td ><%=version.object_changes %></td>
        </tr>
    <% end %>
  </tbody>
</table>
